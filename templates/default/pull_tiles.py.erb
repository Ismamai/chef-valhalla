#!/usr/bin/env python
from __future__ import print_function
import boto

#entry point for script
if __name__ == "__main__":
  #check what we have in s3
  s3 = boto.connect_s3()
  bucket = s3.get_bucket('<%= node[:valhalla][:bucket] %>')
  keys = sorted([ k.key for k in bucket.get_all_keys() if k.key.startswith('<%= node[:valhalla][:bucket_dir] %>' + '/tiles_') if k.key.endswith('tgz') ])
  keys.reverse()
  url = 'https://s3.amazonaws.com/' + '<%= node[:valhalla][:bucket] %>/' + keys[0]
  print(url)
