{
  "mjolnir": {
    "concurrency": <%= node[:valhalla][:mjolnir][:concurrency] %>,
    "tile_dir": "<%= node[:valhalla][:tile_dir] %>",
    "tile_extract": "<%= node[:valhalla][:base_dir] %>/planet.tar",
    "max_cache_size": <%= node[:valhalla][:max_cache_size] %>,
    "statistics": "<%= node[:valhalla][:tile_dir] %>/statistics.sqlite",
    "admin": "<%= node[:valhalla][:tile_dir] %>/admin.sqlite",
    "timezone": "<%= node[:valhalla][:tile_dir] %>/tz_world.sqlite",
    "transit_dir": "<%= node[:valhalla][:transit_dir] %>",
    "logging": {
      "type": "std_out",
      "color": false
    }
  },
  "additional_data": {
    "elevation": "<%= node[:valhalla][:elevation_dir] %>"
  },
  "skadi": {
    "actions": <%= node[:valhalla][:elevation][:actions] %>,
    "logging": {
      "type": "std_out",
      "color": true,
      "long_request": 5.0
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/skadi"
    }
  },
  "loki": {
    "actions": <%= node[:valhalla][:actions] %>,
    "logging": {
      "type": "std_out",
      "color": false,
      "long_request": 100.0
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/loki"
    }
  },
  "thor": {
    "logging": {
      "type": "std_out",
      "color": false,
      "long_request": 110.0
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/thor"
    }
  },
  "odin": {
    "logging": {
      "type": "std_out",
      "color": false
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/odin"
    }
  },
  "tyr": {
    "logging": {
      "type": "std_out",
      "color": false,
      "long_request": 100.0
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/tyr"
    }
  },
  "httpd": {
    "service": {
      "listen": "tcp://<%= node[:valhalla][:httpd][:listen_address] %>:<%= node[:valhalla][:httpd][:port] %>",
      "loopback": "ipc://<%= node[:valhalla][:conf_dir] %>/loopback"
    }
  },
  "service_limits": {
    "auto": {
      "max_distance": 5000000.0,
      "max_locations": 20
    },
    "auto_shorter": {
      "max_distance": 5000000.0,
      "max_locations": 20
    },
    "bus": {
      "max_distance": 5000000.0,
      "max_locations": 50
    },
    "pedestrian": {
      "max_distance": 250000.0,
      "max_locations": 50,
      "min_transit_walking_distance": 1,
      "max_transit_walking_distance": 10000
    },
    "bicycle": {
      "max_distance": 500000.0,
      "max_locations": 50
    },
    "multimodal": {
      "max_distance": 500000.0,
      "max_locations": 50
    },
    "transit": {
      "max_distance": 500000.0,
      "max_locations": 50
    },
    "truck": {
      "max_distance": 5000000.0,
      "max_locations": 20
    },
    "sources_to_targets": {
      "max_distance": 200000.0,
      "max_locations": 50
    },
    "skadi": {
      "max_shape": 750000,
      "min_resample": 10.0
    },
    "isochrone": {
      "max_contours": 4,
      "max_time": 120,
      "max_locations": 1
    },
    "trace_route": {
      "max_shape": 750000
    }
  }
}
